@model IEnumerable<Siparis>

@if (Model.Count() > 0)
{
    @foreach(Siparis siparis in Model)
    {
        <div class="my-3">
            <partial name="_SiparisDetaylari" model="@siparis" />
        </div>
    }
}
else
{
    <div class="alert alert-info">
        <p class="lead">Henüz bir sipariş yok.</p>
    </div>
}